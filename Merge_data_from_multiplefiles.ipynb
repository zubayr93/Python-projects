   "source": [
    "# merging data from multiple files\n",
    "\n",
    "import glob\n",
    "import datetime\n",
    "\n",
    "filenames = glob.glob(\"*.txt\")\n",
    "\n",
    "with open(datetime.datetime.now().strftime(\"%Y-%m-%d-%H-%M\")+\".txt\",'w') as file:\n",
    "    for filename in filenames:\n",
    "        with open(filename,\"r\") as f:\n",
    "            file.write(f.read()+\"\\n\")"
   ]
